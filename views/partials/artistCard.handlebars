<section class="container">
  <h3 class="gallery-name">{{shortStack.name}}</h3>
  <section class="flex-row justify-center">
    <!-- For each painting that belongs to this gallery, the painting-details partial is used to render each painting's data -->
    {{#each shortStack.artwork as |artwork|}}
    {{> artwork.title}}
    {{> artwork.medium}}
    {{> artwork.dimensions}}
    {{> artwork.price}}

    {{/each}}
  </section>
</section>
